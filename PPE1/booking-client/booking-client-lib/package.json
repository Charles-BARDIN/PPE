{
  "name": "booking-client-lib",
  "version": "1.0.0",
  "description": "",
  "main": "index",
  "scripts": {
    "build": "tsc --project ./tsconfig.json && buble -i ./.tmp -o ./dist",
    "prebuild": "npm run -s clean:dist",
    "postbuild": "npm run clean:tmp",
    "build:t": "tsc --project ./tsconfig.spec.json",
    "prebuild:t": "npm run -s prebuild",
    "postbuild:t": "npm run postbuild",    
    "build:w": "tsc -w",
    "prebuild:w": "npm run -s clean:dist",
    "test": "mocha dist/**/*.spec.js",
    "pretest": "npm run -s build:t",
    "clean": "npm run -s clean:dist && npm run clean:tmp",
    "clean:tmp": "rimraf -r ./.tmp/",
    "clean:dist": "rimraf -r ./dist/*"
  },
  "dependencies": {
    "@types/node": "^7.0.8",
    "typescript": "^2.2.2"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.40",
    "buble": "^0.15.2",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
