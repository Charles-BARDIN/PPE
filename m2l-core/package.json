{
  "name": "m2l-core",
  "version": "1.0.0",
  "description": "",
  "main": "index",
  "watch": {
    "build": {
      "patterns": ["src"],
      "extensions": "ts"
    }
  },
  "scripts": {
    "release": "npm run build",
    "prerelease": "npm run -s clean:dist",
    "postrelease": "npm run clean:tmp",
    "build": "tsc --project ./tsconfig.json && buble -i ./.tmp -o ./dist",
    "build:w": "npm-watch",
    "clean": "npm run -s clean:dist && npm run clean:tmp",
    "clean:tmp": "rimraf -r ./.tmp/",
    "clean:dist": "rimraf -r ./dist/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "buble": "^0.15.2",
    "npm-watch": "^0.1.8",
    "rimraf": "^2.6.1",
    "typescript": "^2.2.2"
  }
}
